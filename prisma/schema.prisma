datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int    @id @default(autoincrement())
  firstName String
  lastName  String
  nickName  String @db.VarChar(255)
  password  String @db.VarChar(255)
}
